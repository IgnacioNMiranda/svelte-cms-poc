{
	"name": "svelte-cms-poc",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"pull-cf-schema": "source .env && contentful space export --space-id $CONTENTFUL_SPACE_ID --environment-id $CONTENTFUL_ENVIRONMENT --mt $CONTENTFUL_CMA_TOKEN --skip-content --skip-webhooks --skip-roles --content-file ./scripts/content-types.json",
		"gen-cf-types": "source .env && node ./scripts/gen-cf-types",
		"pull-sb-schema": "source .env && storyblok pull-components --space $STORYBLOK_SPACE_ID",
		"gen-sb-types": "source .env && storyblok-generate-ts source=./components.$STORYBLOK_SPACE_ID.json target=./src/lib/server/cms/storyblok/types"
	},
	"devDependencies": {
		"@contentful/rich-text-types": "16.3.0",
		"@sveltejs/adapter-auto": "3.0.1",
		"@sveltejs/kit": "2.0.6",
		"@types/clone-deep": "4.0.4",
		"@typescript-eslint/eslint-plugin": "6.15.0",
		"@typescript-eslint/parser": "6.15.0",
		"autoprefixer": "10.4.16",
		"cf-content-types-generator": "2.13.0",
		"contentful-cli": "3.1.33",
		"eslint": "8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.35.1",
		"postcss": "8.4.32",
		"prettier": "3.1.1",
		"prettier-plugin-svelte": "3.1.2",
		"storyblok": "3.26.0",
		"storyblok-generate-ts": "2.0.1",
		"svelte": "4.2.8",
		"svelte-check": "3.6.2",
		"tailwindcss": "3.4.0",
		"tslib": "2.6.2",
		"typescript": "5.3.3",
		"vite": "5.0.10"
	},
	"type": "module",
	"dependencies": {
		"@contentful/rich-text-html-renderer": "16.3.0",
		"@storyblok/js": "3.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"clone-deep": "4.0.1",
		"contentful": "10.6.14"
	}
}
